def act_primordial_power_spectrum():
    """ACT prediction for primordial power spectrum."""
    
    # Wavenumber range [Mpc^-1]
    k = np.logspace(-4, 0, 200)
    
    # Standard power-law
    n_s = 0.965  # Spectral index
    A_s = 2.1e-9  # Amplitude
    
    P_prim_lcdm = A_s * (k / 0.05)**(n_s - 1)
    
    # ACT modifications
    P_prim_act = P_prim_lcdm.copy()
    
    # 1. Running spectral index
    alpha_s = -0.004  # Running
    P_prim_act *= (k / 0.05)**(0.5 * alpha_s * np.log(k / 0.05))
    
    # 2. Features from defect formation
    # Oscillatory features at specific scales
    k_features = np.array([1e-3, 3e-3, 1e-2, 3e-2])
    for k_feat in k_features:
        feature = 1 + 0.03 * np.exp(-(np.log(k/k_feat))**2 / (2*0.3**2))
        P_prim_act *= feature
    
    # 3. Cutoff at very large scales
    large_scale = k < 1e-4
    P_prim_act[large_scale] *= (k[large_scale]/1e-4)**2
    
    return k, P_prim_lcdm, P_prim_act

