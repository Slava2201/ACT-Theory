    "# Cell 3: CMB Power Spectrum\n",
    "\n",
    "def act_cmb_power_spectrum(ell_max=2500):\n",
    "    \"\"\"Generate ACT prediction for CMB power spectrum.\"\"\"\n",
    "    \n",
    "    ell = np.arange(2, ell_max)\n",
    "    \n",
    "    D_ell_lcdm = 1000 * ell * (ell + 1) / (2 * np.pi)\n",
    "    \n",
    "    peak_positions = [220, 530, 830, 1120, 1400, 1700, 2000]\n",
    "    peak_amplitudes = [1.0, 0.8, 0.6, 0.4, 0.3, 0.2, 0.15]\n",
    "    \n",
    "    for pos, amp in zip(peak_positions, peak_amplitudes):\n",
    "        idx = np.argmin(np.abs(ell - pos))\n",
    "        width = 50\n",
    "        peak = amp * np.exp(-(ell - pos)**2 / (2 * width**2))\n",
    "        D_ell_lcdm += peak * 3000\n",
    "    \n",
    "    damping = np.exp(-ell / 2000)\n",
    "    D_ell_lcdm *= damping\n",
    "    \n",
    "    D_ell_act = D_ell_lcdm.copy()\n",
    "    \n",
    "    low_ell = ell[ell < 30]\n",
    "    suppression = 0.8 + 0.2 * (low_ell / 30)**2\n",
    "    D_ell_act[ell < 30] *= suppression\n",
    "    \n",
    "    for i, pos in enumerate(peak_positions):\n",
    "        if i % 2 == 0:\n",
    "            idx = np.argmin(np.abs(ell - pos))\n",
    "            D_ell_act[idx-50:idx+50] *= 1.1\n",
    "    \n",
    "    small_scale = ell > 1500\n",
    "    D_ell_act[small_scale] *= 1.05\n",
    "    \n",
    "    return ell, D_ell_lcdm, D_ell_act\n",
    "\n",
    "ell, D_ell_lcdm, D_ell_act = act_cmb_power_spectrum()\n",
    "\n",
