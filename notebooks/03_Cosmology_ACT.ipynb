    "# Cell 4: Large Scale Structure Predictions\n",
    "\n",
    "def act_matter_power_spectrum():\n",
    "    \"\"\"Generate ACT matter power spectrum.\"\"\"\n",
    "    \n",
    "    k = np.logspace(-3, 1, 200)\n",
    "    \n",
    "    P_lcdm = k**1.5 * np.exp(-(k/0.1)**2)\n",
    "    P_lcdm *= (1 + 0.2*np.sin(k/0.07))\n",
    "    \n",
    "    P_act = P_lcdm.copy()\n",
    "    \n",
    "    small_scale = k > 0.1\n",
    "    P_act[small_scale] *= (1 + 0.1 * np.log10(k[small_scale]/0.1))\n",
    "    \n",
    "    bao_shift = 1.02\n",
    "    P_act *= (1 + 0.15*np.sin(k/(0.07*bao_shift)))\n",
    "    \n",
    "    very_small = k > 5\n",
    "    P_act[very_small] *= np.exp(-(k[very_small]-5)**2 / 10)\n",
    "    \n",
    "    return k, P_lcdm, P_act\n",
    "\n",
    "k, P_lcdm, P_act = act_matter_power_spectrum()\n",
    "\n",
    "# Plot LSS predictions\n",
    "fig, axes = plt.subplots(2, 2, figsize=(16, 12))\n",
    "\n",
    "# Plot 1: Matter power spectrum\n",
    "ax1 = axes[0, 0]\n",
    "ax1.loglog(k, P_lcdm, 'r--', linewidth=2, alpha=0.7, label='LambdaCDM')\n",
    "ax1.loglog(k, P_act, 'b-', linewidth=2, label='ACT prediction')\n",
    "\n",
    "ax1.set_xlabel('Wavenumber k [h/Mpc]')\n",
    "ax1.set_ylabel('P(k) [(Mpc/h)^3]')\n",
    "ax1.set_title('Matter Power Spectrum')\n",
    "ax1.grid(True, alpha=0.3, which='both')\n",
    "ax1.legend()\n",
    "\n",
    "# Plot 2: Halo mass function\n",
    "ax2 = axes[0, 1]\n",
    "M = np.logspace(10, 16, 100)\n",
    "sigma = 0.8 * (M/1e12)**(-0.1)\n",
    "nu = 1.686 / sigma\n",
    "dndM_lcdm = 0.3 * np.exp(-nu**2/2) / M**2\n",
    "dndM_act = dndM_lcdm.copy()\n",
    "\n",
    "low_mass = M < 1e12\n",
    "dndM_act[low_mass] *= (1 + 0.3 * (1e12/M[low_mass])**0.1)\n",
    "\n",
    "high_mass = M > 1e14\n",
    "dndM_act[high_mass] *= np.exp(-(M[high_mass]/1e15)**0.5)\n",
    "\n",
    "ax2.loglog(M, dndM_lcdm, 'r--', linewidth=2, alpha=0.7, label='LambdaCDM')\n",
    "ax2.loglog(M, dndM_act, 'b-', linewidth=2, label='ACT prediction')\n",
    "\n",
    "ax2.set_xlabel('Halo Mass M [M_sun/h]')\n",
    "ax2.set_ylabel('dn/dM [Mpc^{-3} M_sun^{-1} h^4]')\n",
    "ax2.set_title('Halo Mass Function')\n",
    "ax2.grid(True, alpha=0.3, which='both')\n",
    "ax2.legend()\n",
    "\n",
    "# Plot 3: Cosmic web slice\n",
    "ax3 = axes[1, 0]\n",
    "n = 64\n",
    "x = np.linspace(-100, 100, n)\n",
    "y = np.linspace(-100, 100, n)\n",
    "z = np.linspace(-100, 100, n)\n",
    "X, Y, Z = np.meshgrid(x, y, z, indexing='ij')\n",
    "\n",
    "r = np.sqrt(X**2 + Y**2 + Z**2)\n",
    "density = np.exp(-r**2/(2*50**2))\n",
    "\n",
    "filament1 = np.exp(-((X-30)**2 + (Y-30)**2)/(2*10**2))\n",
    "filament2 = np.exp(-((X+30)**2 + (Z-30)**2)/(2*10**2))\n",
    "void = -0.5 * np.exp(-((X+50)**2 + Y**2 + Z**2)/(2*30**2))\n",
    "cluster1 = 2.0 * np.exp(-((X-50)**2 + (Y+50)**2 + Z**2)/(2*15**2))\n",
    "cluster2 = 1.5 * np.exp(-((X+50)**2 + (Y-50)**2 + Z**2)/(2*15**2))\n",
    "\n",
    "density += 0.3*filament1 + 0.3*filament2 + void + cluster1 + cluster2\n",
    "density += 0.1 * np.random.randn(*density.shape)\n",
    "\n",
    "z_idx = len(z) // 2\n",
    "density_slice = density[:, :, z_idx]\n",
    "\n",
    "im = ax3.imshow(density_slice.T, extent=[x[0], x[-1], y[0], y[-1]],\n",
    "               cmap='viridis', origin='lower', aspect='auto')\n",
    "ax3.set_xlabel('x [Mpc/h]')\n",
    "ax3.set_ylabel('y [Mpc/h]')\n",
    "ax3.set_title('Cosmic Web Slice (z=0)')\n",
    "plt.colorbar(im, ax=ax3, label='Density contrast delta')\n",
    "\n",
    "# Plot 4: Statistical summary\n",
    "ax4 = axes[1, 1]\n",
    "sigma8_act = np.sqrt(integrate.simps(P_act * k**2, k) / (2*np.pi**2))\n",
    "sigma8_lcdm = np.sqrt(integrate.simps(P_lcdm * k**2, k) / (2*np.pi**2))\n",
    "\n",
    "S8_act = sigma8_act * np.sqrt(0.3)\n",
