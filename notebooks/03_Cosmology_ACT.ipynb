    "ax3.set_title('Density Component Evolution')\n",
    "ax3.grid(True, alpha=0.3, which='both')\n",
    "ax3.set_ylim([1e-8, 10])\n",
    "ax3.legend()\n",
    "\n",
    "# Plot 4: Hubble parameter evolution\n",
    "ax4 = axes[1, 1]\n",
    "def hubble_act(z, h0=0.7):\n",
    "    \"\"\"ACT Hubble parameter.\"\"\"\n",
    "    a = 1/(1+z)\n",
    "    omega_total = 1e-4*a**(-4) + 0.3*a**(-3) + 0.268*a**(-1.5) + 0.7*(1+0.05*(1-a))\n",
    "    return h0 * 100 * np.sqrt(omega_total)\n",
    "\n",
    "def hubble_lcdm(z, h0=0.7):\n",
    "    \"\"\"Standard LCDM Hubble parameter.\"\"\"\n",
    "    a = 1/(1+z)\n",
    "    omega_total = 1e-4*a**(-4) + 0.3*a**(-3) + 0.7\n",
    "    return h0 * 100 * np.sqrt(omega_total)\n",
    "\n",
    "H_act = np.array([hubble_act(zi) for zi in z])\n",
    "H_lcdm = np.array([hubble_lcdm(zi) for zi in z])\n",
    "\n",
    "ax4.loglog(z, H_act, 'b-', linewidth=2, label='ACT')\n",
    "ax4.loglog(z, H_lcdm, 'r--', linewidth=2, label='LambdaCDM')\n",
    "ax4.set_xlabel('Redshift z')\n",
    "ax4.set_ylabel('H(z) [km/s/Mpc]')\n",
    "ax4.set_title('Hubble Parameter Evolution')\n",
    "ax4.grid(True, alpha=0.3, which='both')\n",
    "ax4.legend()\n",
    "\n",
