    "ax3 = axes[1, 0]\n",
    "z_dens = np.logspace(-3, 8, 200)\n",
    "a_dens = 1/(1+z_dens)\n",
    "\n",
    "omega_r = 1e-4 * a_dens**(-4)\n",
    "omega_m = 0.3 * a_dens**(-3)\n",
    "omega_lambda = 0.7 * np.ones_like(z_dens)\n",
    "omega_defects = 0.268 * a_dens**(-1.5)\n",
    "omega_causal = 0.7 * (1 + 0.05*(1-a_dens))\n",
    "\n",
    "ax3.loglog(z_dens, omega_r, 'c-', linewidth=2, label='Radiation')\n",
    "ax3.loglog(z_dens, omega_m, 'g-', linewidth=2, label='Baryonic Matter')\n",
    "ax3.loglog(z_dens, omega_defects, 'b-', linewidth=2, label='ACT Dark Matter')\n",
    "ax3.loglog(z_dens, omega_causal, 'r-', linewidth=2, label='ACT Dark Energy')\n",
