# Hubble parameter H(z)
def hubble_act(z, h0=0.7):
    """ACT Hubble parameter."""
    a = 1/(1+z)
    # Modified Friedmann equation
    omega_total = 1e-4*a**(-4) + 0.3*a**(-3) + 0.268*a**(-1.5) + 0.7*(1+0.05*(1-a))
    return h0 * 100 * np.sqrt(omega_total)

def hubble_lcdm(z, h0=0.7):
    """Standard LCDM Hubble parameter."""
    a = 1/(1+z)
    omega_total = 1e-4*a**(-4) + 0.3*a**(-3) + 0.7
    return h0 * 100 * np.sqrt(omega_total)

H_act = np.array([hubble_act(zi) for zi in z])
