    "epochs, temperatures, act_features = act_cosmic_timeline()\n",
    "\n",
    "# Plot cosmic timeline\n",
    "fig, axes = plt.subplots(2, 2, figsize=(16, 12))\n",
    "\n",
    "# Plot 1: Temperature evolution\n",
