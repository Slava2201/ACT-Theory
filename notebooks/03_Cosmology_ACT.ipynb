def act_cmb_power_spectrum(ell_max=2500):
    """Generate ACT prediction for CMB power spectrum."""
    
    # Multipole range
    ell = np.arange(2, ell_max)
    
    # Base LCDM spectrum (simplified)
    # Sachs-Wolfe plateau
    D_ell_lcdm = 1000 * ell * (ell + 1) / (2 * np.pi)
    
    # Acoustic peaks
    peak_positions = [220, 530, 830, 1120, 1400, 1700, 2000]
    peak_amplitudes = [1.0, 0.8, 0.6, 0.4, 0.3, 0.2, 0.15]
    
    for pos, amp in zip(peak_positions, peak_amplitudes):
        idx = np.argmin(np.abs(ell - pos))
        width = 50
        peak = amp * np.exp(-(ell - pos)**2 / (2 * width**2))
        D_ell_lcdm += peak * 3000
    
    # Damping tail
    damping = np.exp(-ell / 2000)
    D_ell_lcdm *= damping
    
    # ACT modifications
    D_ell_act = D_ell_lcdm.copy()
    
    # 1. Low-ell suppression (ell < 30)
    low_ell = ell[ell < 30]
    suppression = 0.8 + 0.2 * (low_ell / 30)**2
    D_ell_act[ell < 30] *= suppression
    
    # 2. Enhanced oscillations (ACT predicts different phase)
    for i, pos in enumerate(peak_positions):
        if i % 2 == 0:  # Even peaks enhanced
            idx = np.argmin(np.abs(ell - pos))
            D_ell_act[idx-50:idx+50] *= 1.1
    
    # 3. Small-scale enhancement from defects
    small_scale = ell > 1500
    D_ell_act[small_scale] *= 1.05
    
    return ell, D_ell_lcdm, D_ell_act

