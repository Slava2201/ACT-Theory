    "\n",
    "def act_scale_factor(z):\n",
    "    \"\"\"ACT modified scale factor evolution.\"\"\"\n",
    "    a = 1/(1+z)\n",
    "    if z > 1e6:\n",
    "        return a * (1 + 0.1*np.exp(-z/1e7))\n",
    "    return a\n",
    "\n",
    "a_act = np.array([act_scale_factor(zi) for zi in z])\n",
    "a_lcdm = 1/(1+z)\n",
