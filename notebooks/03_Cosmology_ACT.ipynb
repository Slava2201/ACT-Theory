def act_matter_power_spectrum():
    """Generate ACT matter power spectrum."""
    
    # Wavenumber range [h/Mpc]
    k = np.logspace(-3, 1, 200)
    
    # Base Î›CDM power spectrum
    P_lcdm = k**1.5 * np.exp(-(k/0.1)**2)  # Primordial
    P_lcdm *= (1 + 0.2*np.sin(k/0.07))  # BAO wiggles
    
    # ACT modifications
    P_act = P_lcdm.copy()
    
    # 1. Enhanced small-scale power (defect clustering)
    small_scale = k > 0.1
    P_act[small_scale] *= (1 + 0.1 * np.log10(k[small_scale]/0.1))
    
    # 2. Modified BAO (different sound horizon)
    bao_shift = 1.02  # 2% shift
    P_act *= (1 + 0.15*np.sin(k/(0.07*bao_shift)))
    
    # 3. Cutoff at very small scales
    very_small = k > 5
    P_act[very_small] *= np.exp(-(k[very_small]-5)**2 / 10)
    
    return k, P_lcdm, P_act

